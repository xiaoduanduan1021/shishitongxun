<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.clint.shishitongxun.xinxi.model.Guke" table="guke">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment"/>
		</id>
		
		<property name="httpSessionId" column="httpSessionId" type="string" />
		<property name="kefuId" column="kefuId" type="integer" />
		<property name="zhuceShijian" column="zhuceShijian" type="string" />
		<property name="beizhu" column="beizhu" type="string" />
		<property name="newXinxi" column="newXinxi" type="integer" />
		
		<!-- ip -->
		<property name="ip" column="ip" type="string" />
		<!-- 国家 -->
		<property name="country" column="country" type="string" />
		<!-- 国家id -->
		<property name="country_id" column="country_id" type="string" />
		<!-- 地区 -->
		<property name="area" column="area" type="string" />
		<!-- 地区id -->
		<property name="area_id" column="area_id" type="string" />
		<!-- 区域 -->
		<property name="region" column="region" type="string" />
		<!-- 区域id -->
		<property name="region_id" column="region_id" type="string" />
		<!-- 城市 -->
		<property name="city" column="city" type="string" />
		<!-- 城市id -->
		<property name="city_id" column="city_id" type="string" />
		<!-- 县 -->
		<property name="county" column="county" type="string" />
		<!-- 县id -->
		<property name="county_id" column="county_id" type="string" />
		<!-- Internet服务提供商 -->
		<property name="isp" column="isp" type="string" />


		<property name="accessSessionId" column="accessSessionId" type="string" />
		
		
	</class>
</hibernate-mapping>
